<div class ="songs-container">

<h1> Add New Song </h2>

<%= form_for @songs do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title, required:true, autofocus: true %>
  <br />
  <%= f.label :album %>
  <%= f.text_field :album, required:true, autofocus: true %>
  <br />
  
  
  <%= f.submit %>

<% end %> 
</div>